{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": false
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-undef": "off",
    "padded-blocks": "off",
    "max-len": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended", "google"]
}